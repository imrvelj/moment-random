{
  "name": "moment-random",
  "version": "1.0.4",
  "author": "Ivan Mrvelj <ivan.mrvelj@protonmail.com>",
  "description": "Generates a random momentjs object",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:imrvelj/moment-random.git",
    "type": "git"
  },
  "main": "src/index.js",
  "scripts": {
    "precommit": "npm test",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "files": [
    "src",
    "README.md"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/*.js"
    ],
    "testMatch": [
      "**/test/**/*.spec.js?(x)"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-jest": "^19.0.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^3.9.1",
    "eslint-plugin-jest": "^19.0.1",
    "husky": "^0.13.3",
    "jest": "^23.0.0",
    "moment": "^2.20.1",
    "rimraf": "^2.6.1"
  },
  "peerDependencies": {
    "moment": ">= 2"
  }
}
