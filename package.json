{
  "name": "moment-random",
  "version": "1.1.0",
  "author": "Ivan Mrvelj <imrvelj@protonmail.com>",
  "description": "Generates a random momentjs object",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:imrvelj/moment-random.git",
    "type": "git"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "files": [
    "src",
    "README.md"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/*.js"
    ],
    "testMatch": [
      "**/test/**/*.spec.js?(x)"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "jest": "^24.7.1",
    "moment": "^2.24.0"
  },
  "peerDependencies": {
    "moment": ">= 2"
  }
}
